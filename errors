Generated SQL:

SELECT *
FROM (
  SELECT 'crew_roster.reserveslegality' AS source_table,
         CAST(rl.ReservesCrewMemberID AS TEXT) AS employee_id,
         NULL AS first_name,
         NULL AS last_name,
         'ReservesLegalityID=' || rl.ReservesLegalityID ||
         ', BaseDateID=' || rl.BaseDateID ||
         ', SequencePositionDetailsID=' || rl.SequencePositionDetailsID ||
         ', StandByID=' || rl.StandByID ||
         ', MovableDays=' || rl.MovableDays ||
         ', LineHolderDays=' || rl.LineHolderDays AS details
  FROM crew_roster.reserveslegality rl
  WHERE CAST(rl.ReservesCrewMemberID AS TEXT) IN ('101','AA-101')

  UNION ALL

  SELECT 'compliance_training.compliance_checks' AS source_table,
         CAST(cc.crew_id AS TEXT) AS employee_id,
         NULL AS first_name,
         NULL AS last_name,
         'check_id=' || cc.check_id ||
         ', check_type=' || cc.check_type ||
         ', check_date=' || cc.check_date ||
         ', result=' || cc.result ||
         ', next_due_date=' || cc.next_due_date ||
         ', examiner_name=' || cc.examiner_name ||
         ', is_overdue=' || cc.is_overdue ||
         ', remarks=' || COALESCE(cc.remarks,'') AS details
  FROM compliance_training.compliance_checks cc
  WHERE CAST(cc.crew_id AS TEXT) IN ('101','AA-101')

  UNION ALL

  SELECT 'crew_management.crew_documents' AS source_table,
         CAST(cd.crew_id AS TEXT) AS employee_id,
         NULL AS first_name,
         NULL AS last_name,
         'document_id=' || cd.document_id ||
         ', document_type=' || cd.document_type ||
         ', document_name=' || cd.document_name ||
         ', upload_date=' || cd.upload_date ||
         ', expiry_date=' || cd.expiry_date ||
         ', verified=' || cd.verified ||
         ', verified_by=' || COALESCE(cd.verified_by,'') ||
         ', file_path=' || COALESCE(cd.file_path,'') AS details
  FROM crew_management.crew_documents cd
  WHERE CAST(cd.crew_id AS TEXT) IN ('101','AA-101')

  UNION ALL

  SELECT 'compliance_training.audit_logs' AS source_table,
         CAST(al.entity_id AS TEXT) AS employee_id,
         NULL AS first_name,
         NULL AS last_name,
         'log_id=' || al.log_id ||
         ', entity_type=' || al.entity_type ||
         ', action=' || al.action ||
         ', performed_by=' || al.performed_by ||
         ', ip_address=' || COALESCE(al.ip_address,'') ||
         ', performed_at=' || al.performed_at ||
         ', old_value=' || COALESCE(al.old_value,'') ||
         ', new_value=' || COALESCE(al.new_value,'') AS details
  FROM compliance_training.audit_logs al
  WHERE CAST(al.entity_id AS TEXT) IN ('101','AA-101')
     OR al.old_value LIKE '%101%'
     OR al.new_value LIKE '%101%'
) x
LIMIT 100;


SELECT
  cm.employee_id,
  cm.crew_id,
  cm.first_name,
  cm.last_name,
  cm.email,
  cm.phone,
  cm.date_of_birth,
  cm.nationality,
  cm.passport_number,
  cm.passport_expiry,
  cm.hire_date,
  cm.seniority_number,
  cm.crew_role,
  cm.base_airport,
  cm.status,
  rcm.EmployeeID AS roster_employee_numeric_id,
  rcm.ReservesCrewMemberID,
  rcm.FirstName AS roster_first_name,
  rcm.LastName AS roster_last_name,
  rcm.SeniorityNumber AS roster_seniority_number,
  rcm.BaseDateID,
  rcm.AVLDays,
  rcm.AVLDaysWithFT,
  rcm.ASGDays,
  rcm.ASGDaysWithFT,
  rcm.ASGSequence,
  rcm.ASGStandby,
  rcm.ETBonFD,
  rcm.IsVolunteer,
  rcm.IsSick,
  rcm.IsCompletelyAvailableReserve,
  pr.payroll_id,
  pr.pay_period_start,
  pr.pay_period_end,
  pr.base_pay,
  pr.flight_hours,
  pr.flight_hour_pay,
  pr.per_diem_days,
  pr.per_diem_pay,
  pr.overtime_hours,
  pr.overtime_pay,
  pr.deductions,
  pr.tax_withheld,
  pr.net_pay,
  pr.payment_date,
  pr.payment_status,
  lb.balance_id,
  lb.leave_type,
  lb.year AS leave_year,
  lb.entitled_days,
  lb.used_days,
  lb.pending_days,
  lb.carried_over,
  tr.record_id AS training_record_id,
  tr.course_id,
  tr.training_date,
  tr.completion_date,
  tr.score,
  tr.result AS training_result,
  tr.instructor_name,
  tr.facility,
  tr.certificate_number,
  tr.expiry_date AS training_expiry_date,
  tr.notes AS training_notes,
  cc.check_id AS compliance_check_id,
  cc.check_type,
  cc.check_date,
  cc.result AS compliance_result,
  cc.next_due_date,
  cc.examiner_name,
  cc.remarks AS compliance_remarks,
  cc.is_overdue,
  si.incident_id,
  si.flight_id AS incident_flight_id,
  si.incident_date,
  si.incident_type,
  si.severity,
  si.description AS incident_description,
  si.corrective_action,
  si.investigation_status,
  si.resolution_date
FROM crew_management.crew_members cm
LEFT JOIN crew_roster.reservescrewmember rcm
  ON CAST(rcm.EmployeeID AS TEXT) = REPLACE(cm.employee_id, 'AA-', '')
LEFT JOIN hr_payroll.payroll_records pr
  ON pr.crew_id = cm.crew_id
LEFT JOIN hr_payroll.leave_balances lb
  ON lb.crew_id = cm.crew_id
LEFT JOIN compliance_training.training_records tr
  ON tr.crew_id = cm.crew_id
LEFT JOIN compliance_training.compliance_checks cc
  ON cc.crew_id = cm.crew_id
LEFT JOIN compliance_training.safety_incidents si
  ON si.reported_by_crew_id = cm.crew_id
WHERE cm.employee_id = 'AA-101'
   OR CAST(rcm.EmployeeID AS TEXT) = '101'
LIMIT 100;

